{
  "name": "tiny-office",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-tinyoffice-cap": "cds watch --open tiny.office.tinyofficecap/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-tinyoffice": "cds watch --open tinyoffice.tinyoffice/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/tinyoffice-cap",
    "app/tinyoffice"
  ]
}
